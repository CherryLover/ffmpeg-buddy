{
  "private": true,
  "scripts": {
    "pretest": "npm run lint",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"**/*.js\"",
    "lint:prettier": "prettier --check \"**/*{md,html,css,js,json}\"",
    "format": "prettier --write \"**/*{md,html,css,js,json}\"",
    "test": "mocha spec/**/*.js",
    "start": "brunch watch --server",
    "build": "brunch build --production"
  },
  "dependencies": {
    "knockout": "^3.5.1",
    "knockout-es5": "^0.4.5"
  },
  "devDependencies": {
    "brunch": "^4.0.0",
    "clean-css-brunch": "^3.0.0",
    "css-brunch": "^2.10.0",
    "eslint": "^8.0.0",
    "javascript-brunch": "^2.10.0",
    "mocha": "^9.1.2",
    "prettier": "^2.4.1",
    "uglify-js-brunch": "^2.10.0"
  },
  "standard": {
    "globals": [
      "describe",
      "beforeEach",
      "afterEach",
      "it"
    ]
  }
}
